source /opt/ros/humble/setup.bash
cd ~/ros2_ws/src


# RUN
ros2 run PACKAGE NODE

# Rviz
ros2 run rviz2 rviz2

# Create new Package
ros2 pkg create --build-type ament_cmake NAME

# BUILD
colcon build --packages-select PACKAGE
. install/setup.bash



